{
  "branches": ["master", "main", "features/auto-deploy"],
  "repositoryUrl": "git@github.com:jan-molak/playwright-ct-web.git",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [ "./semantic-release-version-mirror", {
        "pathToPackageJson": "./playwright-ct-web/package.json",
        "dependencyToMirror": "@playwright/experimental-ct-core"
    } ],
    [ "@semantic-release/npm", {
        "npmPublish": false
    } ],
    "@semantic-release/github"
  ],
  "ci": false,
  "debug": true,
  "dryRun": true
}
