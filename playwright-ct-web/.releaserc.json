{
  "branches": ["master", "main", "features/auto-deploy"],
  "repositoryUrl": "git@github.com:jan-molak/playwright-ct-web.git",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [ "../semantic-release-version-mirror", {
        "dependency": "@playwright/experimental-ct-core"
    } ],
    [ "@semantic-release/npm", {
        "npmPublish": false
    } ],
    [ "@semantic-release/github", {
      "assets": [
        { "path": "*.tgz", "label": "playwright-ct-web" }
      ]
    } ]
  ],
  "ci": true,
  "debug": true,
  "dryRun": false
}
