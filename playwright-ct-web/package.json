{
  "name": "@sand4rt/experimental-ct-web",
  "version": "1.51.1",
  "description": "Playwright Component Testing for Web Components",
  "homepage": "https://playwright.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jan-molak/playwright-ct-web.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/sand4rt/playwright-ct-web/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "sand4rt"
  },
  "license": "MIT",
  "keywords": [
    "testing",
    "web components",
    "real browser",
    "playwright",
    "unit",
    "integration",
    "functional",
    "end to end",
    "e2e"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./register": {
      "types": "./register.d.ts",
      "default": "./register.mjs"
    },
    "./hooks": {
      "types": "./hooks.d.ts",
      "default": "./hooks.mjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@playwright/experimental-ct-core": "1.51.1"
  },
  "bin": {
    "playwright": "cli.js"
  }
}
